    };
    return nepali_keywords[keyword];
} // end keyword_translate()

var count = 0;
var childcount = 0;
var cursor = db.tags.find();
while (cursor.hasNext()) {
    var tag = cursor.next();
    count++;
    tag['ndn'] = keyword_translate(tag['name']);
    //print(Object.keys(tag));
    if (tag['children'])
        //print(Object.keys(tag['children']));
        for (var kid in tag['children']) {
        //tag['children'].foreach (function(kid,index) {
            childcount++;
            //print ( tag['children'][kid].name);
            tag['children'][kid].ndn =  keyword_translate(tag['children'][kid].name);
        print(tag.name + ' contains NAME    ' + tag['children'][kid].name + '     TRANSLATES TO  ' + tag['children'][kid].ndn);
    };
    
    db.tags.update({_id:tag._id}, tag);
    }
print('processed ' + count + ' keywords');
print(' and processed ' + childcount + ' children');
print();
